//COLORES FR1K1.COM
frblue = #049DD9
//Estilos de Blog-news
section.Blog-news
	border-bottom 3px solid frblue
	height auto
	width 100%
	padding 0px
	/*margin-top 95px*/
	div.Container-columnas
		-webkit-flex-flow row wrap
		display -moz-box
		display -ms-flexbox
		display -webkit-box
		display -webkit-flex
		display inline-flex
		height auto
		justify-content center
		width 100%
		div.Columna1, 
		div.Columna2, 
		div.Columna3, 
		div.Columna4, 
		div.Columna5, 
		div.Columna6
			margin 15px
			padding 0
			width 310px
			background black
			box-shadow 0 0 0 10px white,
          			   0 0 0 11px frblue,
          			   0 20px 20px -12px black
			a.Noticia1-link
				background steelblue
				height auto
				margin 0px
				padding 0px
				width 310px
				text-decoration none
				article.Noticia1-article
					width 100%
					height auto
					header.Noticia1-portada
						width 100%
						height auto
						img.Noticia1-portadaImg
							width 310px
							height auto
							margin 0px auto 
					div.Noticia1-info
						-webkit-flex-flow row
						display -moz-box
						display -ms-flexbox
						display -webkit-box
						display -webkit-flex
						display flex
						height 30px
						width 100%
						img.Noticia1-seccion
							width 20px
							padding 0px 3px 0px 3px
							margin 0px 1px 0px 1px
							line-height 30px
						h3.Noticia1-title2
							font-size 1.2em
							margin 0px 1px 0px 1px
							padding 0px 3px 0px 3px
							line-height 30px
							color white							
					h2.Noticia1-title1
						margin 0px 4px 0px 4px
						padding 0px
						color white
					p.Noticia1-intro
						margin 0px 4px 0px 4px
						padding 0px
						color white
					span.Noticia1-date
						font-size .8em
						margin 0px 1px 0px 1px
						padding 0px 3px 0px 3px
						line-height 30px
						color orange
						text-align center
			div.Noticia1-btnCompartir
				width 100%
				height auto
				padding 0px
				margin 0px
				-webkit-flex-flow row wrap
				display -moz-box
				display -ms-flexbox
				display -webkit-box
				display -webkit-flex
				display inline-flex
				justify-content space-around
				align-items center
				background white
				div.Noticia1-divHeader
					width 100%
					height 100%
					padding 0px
					margin 0px
					text-align center
					img.Noticia1-logofont
						width 160px
						padding 10px 5px 0px 5px
						margin-right auto
						margin-left auto
						line-height 160px
				a.Noticia1-btnlink1, a.Noticia1-btnlink2
					width 50%
					height auto
					padding 0px
					margin 0px
					text-align center
					color solid white
					img.Noticia1-btnImg
						width 30px
						margin 0px 10px
						padding 0px
				a.Noticia1-btnlink1
					text-align right
				a.Noticia1-btnlink2
					text-align left
