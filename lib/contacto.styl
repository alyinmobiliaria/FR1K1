//COLORES FR1K1.COM
frblue = #049DD9
//MODULO: Estilos Contacto
html
	width 100%
	min-height 1008px
body.Contacto
	width 100%
	min-height 1008px
	background white
	font-family Oswald
	-webkit-flex-flow column
	display -moz-box
	display -ms-flexbox
	display -webkit-box
	display -webkit-flex
	display flex
	justify-content space-around
	margin-right auto
	margin-left auto
	list-style none
	box-sizing border-box
	align-items center
	div.Contacto-logoContainer
		min-height 20%
		padding auto
		margin auto
		text-align center
		img.Form-logoFont
			width 300px
			margin-right auto
			margin-left auto
			padding 0px
	div.Contacto-pContainer
		min-height 20%
		padding auto
		margin auto
		p
			width 300px
			margin-right auto
			margin-left auto
			margin-top 23px
			font-size 1em
			text-align left
	div.Contacto-form 
		min-height 20%  
		padding auto
		margin auto 
		form.Contacto-formContainer
			width 100%
			height 100%
			background white
			margin-right auto
			margin-left auto
			padding 0px
			text-align center
			-webkit-flex-flow column
			display -moz-box
			display -ms-flexbox
			display -webkit-box
			display -webkit-flex
			display flex
			justify-content center
			margin-right auto
			margin-left auto
			list-style none
			box-sizing border-box
			align-items center
			bottom 0px
			input, textarea
				width 300px
				margin-bottom 20px
				padding 7px
				box-sizing border-box
				border none
				font-size 1em
				margin-right auto
				margin-left auto
				margin-top 23px
				background frblue
				font-weight bold
				color white
				&:hover
					background lightgray
					color black
			textarea
				min-height 100px
				max-height 200px
				max-width 100%
				resize none
	div.Contacto-ulContainer
		min-height 20%
		padding auto
		margin auto
		ul.container
			// border-top 3px solid frblue
			height 40px
			list-style none
			margin auto
			padding 0px
			width 300px
			display -webkit-box
			display -moz-box
			display -ms-flexbox
			display -webkit-flex
			display flex
			-webkit-flex-flow row
			li.flex-item
				background white/*rgba(0, 0, 0, 0.8)*/
				height 100%
				line-height 100%
				text-align center
				width 15%
				&:hover
					background lightgray
				a.Link
					width 100%				
					img.Nav_img
						padding 0px
						margin 0px
						width 30px	
